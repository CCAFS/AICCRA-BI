    <connection>ORI_AICCRA</connection>
    <sql>SELECT 
-- 	gu.acronym AS "CRP",
pin.id as innovation_id,
	CONCAT(
		'https://aiccra.marlo.cgiar.org/clusters/AICCRA/innovation.do?innovationID=',
		pin.id,
		'&amp;edit=true&amp;phaseID=',
		miph.max_id_phase -- ,
		-- '"),"PI',
		-- pin.id
		-- ,
		-- '")'
	) AS "link_innovation_id",
	pini.year as innovation_year,
	(
		CASE
		WHEN ISNULL(pini.title)
		OR pini.title = "" THEN
			"&lt;Not provided>"
		ELSE
			pini.title
		END
	) AS "innovation_title",
	(
		CASE
		WHEN ISNULL(risi.NAME) THEN
			"&lt;Not provided>"
		ELSE
			risi.NAME
		END
	) AS "innovation_stage",
		(
		CASE
		WHEN ISNULL(risi.definition) THEN
			"&lt;Not provided>"
		ELSE
			risi.definition
		END
	) AS "innovation_stage_definition",
	(
		CASE
		WHEN ISNULL(riit.`name`) THEN
			"&lt;Not provided>"
		ELSE
			riit.`name`
		END
	) AS "innovation_type",
	(
		CASE
		WHEN ISNULL(rigs.`name`) THEN
			"&lt;Not provided>"
		ELSE
			rigs.`name`
		END
	) AS "geographic_scope",
	(
		CASE
		WHEN ISNULL(rigs.`name`)
		OR rigs.id != 2 THEN
			"&lt;Not Applicable>"
		WHEN ISNULL(rir.`name`) THEN
			"&lt;Not provided>"
		ELSE
			rir.`name`
		END
	) AS "region",
	(
		CASE
		WHEN ISNULL(rigs.`name`)
		OR rigs.id = 1
		OR rigs.id = 2 THEN
			"&lt;Not Applicable>"
		WHEN ISNULL(le.`name`) THEN
			"&lt;Not Defined>"
		ELSE
			 le.`name`
		END
	) AS "country",
	(
		CASE
		WHEN ISNULL(rigender.`name`) THEN
			"&lt;Not provided>"
		ELSE
			rigender.`name`
		END
	) AS 'gender_relevance',
	(
		CASE
		WHEN ISNULL(pini.gender_explaniation)
		OR pini.gender_explaniation = "" THEN
			"&lt;Not provided>"
		ELSE
			pini.gender_explaniation
		END
	) AS "gender_explanation_evidence",
	(
		CASE
		WHEN ISNULL(riyouth.`name`) THEN
			"&lt;Not provided>"
		ELSE
			riyouth.`name`
		END
	) AS 'youth_relevance',
	(
		CASE
		WHEN ISNULL(pini.youth_explaniation)
		OR pini.youth_explaniation = "" THEN
			"&lt;Not provided>"
		ELSE
			pini.youth_explaniation
		END
	) AS "youth_explanation_evidence"
 	,
 	cpo.id as indicator_id
 	,
	cpo.acronym as indicator_acronym
	,
	cpo.description  as indicator_name
	,
	inst.institution_name
	,inst.institution_role
	,ifnull(inst.center_name, 'Not applicable') as center_name
	,ifnull(inst.contributor_name, 'Not applicable') as contributor_name
-- 	(
-- 		CASE
-- 		WHEN ISNULL(pini.evidence_link)
-- 		OR pini.evidence_link = "" THEN
-- 			"&lt;Not Defined>"
-- 		ELSE
-- 			pini.evidence_link
-- 		END
-- 	) AS "Evidence Link"
,ifnull(concat(cpo.composed_id,
                                        '_',
                                        pini.`year` 
                        ),'NP') indicator_pk
        ,   ifnull(concat(pin.project_id,'_',cpo.composed_id,
                                        '_',
                                        pini.`year`
                        ),'NP') contribution_pk	
, pesp1.project_id cluster_id
, pesp1.cluster_owner_type
, pesp1.cluster_owner_id
, p.acronym cluster_acronym
, pown.acronym cluster_owner_acronym
, sysdate() AS updated_date
, 'Not applicable' status
, pi2.title cluster_title 
, sclu.shared_clusters_acronym
FROM phases ph
-- INNER JOIN global_units gu ON gu.id = ph.global_unit_id
-- AND gu.is_active
INNER JOIN (select gu1.id gu_id, phg.id max_id_phase, phg.name name_max_id_phase , phg.year year_max_id_phase, phg.description desc_max_id_phase
from global_units gu1 
join phases phg on phg.global_unit_id = gu1.id 
where gu1.is_active
and phg.id = ifnull(get_max_open_phase(gu1.id),0)
) miph on miph.gu_id = ph.global_unit_id -- gu.id
-- INNER JOIN projects_info pi ON pi.id_phase = ph.id and pi.is_active
-- INNER JOIN projects p ON p.id = pi.project_id AND p.is_active
INNER JOIN (select parameter_name, cast(parameter_value as double) parameter_value from bi_parameters where parameter_name='BI_data_phase' ) bpacc on bpacc.parameter_name= 'BI_data_phase'
INNER JOIN project_innovations pin ON pin.is_active
INNER JOIN project_innovation_info pini ON pini.project_innovation_id = pin.id AND pini.id_phase = ph.id
-- projects: owner and shared
inner join (select pii.id_phase, pin1.id, pin1.project_id , "Owner" cluster_owner_type, pin1.project_id cluster_owner_id
			FROM project_innovations pin1	
			inner join bi_parameters bpa1 on bpa1.parameter_name='BI_data_phase'
			INNER JOIN project_innovation_info pii ON pii.project_innovation_id = pin1.id 
			where pin1.is_active 
			and pii.id_phase = cast(bpa1.parameter_value as double)
			union 
			select pis.id_phase, pin1.id, pis.project_id project_id , "Shared" cluster_owner_type, pin1.project_id cluster_owner_id
			from project_innovations pin1
			inner join bi_parameters bpa1 on bpa1.parameter_name='BI_data_phase'
			inner JOIN project_innovation_shared pis on pis.project_innovation_id = pin1.id and pis.is_active 
			and pis.id_phase =cast(bpa1.parameter_value as double)
			where pin1.is_active
			 -- bpa1.parameter_value 
			and pis.project_id != pin1.project_id) pesp1 on pesp1.id_phase = pini.id_phase  and pesp1.id = pini.project_innovation_id  
left join projects p on p.id = pesp1.project_id and p.is_active 
left join projects_info pi2 on pi2.project_id = p.id and pi2.id_phase = ph.id and pi2.is_active 
left join projects pown on pown.id = pesp1.cluster_owner_id and pown.is_active 
LEFT JOIN rep_ind_stage_innovations risi ON risi.id = pini.stage_innovation_id
LEFT JOIN rep_ind_innovation_types riit ON riit.id = pini.innovation_type_id
LEFT JOIN rep_ind_contribution_of_crp ricc ON ricc.id = pini.rep_ind_contribution_crp_id
LEFT JOIN project_innovation_geographic_scopes pigs on pigs.project_innovation_id = pini.project_innovation_id and pigs.id_phase = pini.id_phase 
LEFT JOIN rep_ind_geographic_scopes rigs ON rigs.id = pigs.rep_ind_geographic_scope_id 
LEFT JOIN project_innovation_regions pir on pir.project_innovation_id = pini.project_innovation_id and pir.id_phase = pini.id_phase 
LEFT JOIN loc_elements rir ON rir.id = pir.id_region 
LEFT JOIN project_innovation_countries pesc ON pesc.project_innovation_id = pini.project_innovation_id  AND pesc.id_phase = pini.id_phase 
LEFT JOIN loc_elements le ON le.id = pesc.id_country
LEFT JOIN rep_ind_gender_youth_focus_levels rigender ON rigender.id = pini.gender_focus_level_id
LEFT JOIN rep_ind_gender_youth_focus_levels riyouth ON riyouth.id = pini.youth_focus_level_id
LEFT JOIN project_innovation_crp_outcomes pico2 on pico2.project_innovation_id = pini.project_innovation_id and pico2.id_phase = pini.id_phase 
LEFT JOIN crp_program_outcomes cpo on cpo.id = pico2.crp_outcome_id 
LEFT JOIN (select pini1.project_innovation_id, pini1.id_phase, 
			ifnull(concat(if (ifnull(ice.acronym,'') != '', concat(ice.acronym,'-'), ''), ice.name),'Not Provided') institution_name, 
			'Center/Managing' as institution_role, 
			ifnull(concat(if (ifnull(ice.acronym,'') != '', concat(ice.acronym,'-'), ''), ice.name),'Not Provided') center_name, '' contributor_name
		   from project_innovations pin1 
		   inner join bi_parameters bpa1 on bpa1.parameter_name='BI_data_phase'
		   INNER JOIN phases ph1 on ph1.id = cast(bpa1.parameter_value as double)
			INNER JOIN project_innovation_info pini1 ON pini1.project_innovation_id = pin1.id AND pini1.id_phase = ph1.id
		   	INNER JOIN project_innovation_centers pic on pic.project_innovation_id = pini1.project_innovation_id and pic.id_phase = pini1.id_phase 
			INNER JOIN institutions ice on ice.id = pic.institution_id 
			UNION 
			select pini1.project_innovation_id, pini1.id_phase, 
			ifnull(concat(if (ifnull(io.acronym,'') != '' , concat(io.acronym,'-'), ''), io.name),'Not Provided') institution_name, 
			'Contributor' as institution_role, 
			'' center_name, 
			ifnull(concat(if (ifnull(io.acronym,'') != '', concat(io.acronym,'-'), ''), io.name),'Not Provided') contributor_name
		   from project_innovations pin1 
		   inner join bi_parameters bpa1 on bpa1.parameter_name='BI_data_phase'
		   INNER JOIN phases ph1 on ph1.id = cast(bpa1.parameter_value as double)
			INNER JOIN project_innovation_info pini1 ON pini1.project_innovation_id = pin1.id AND pini1.id_phase = ph1.id
			INNER JOIN project_innovation_contributing_organizations pico on pico.project_innovation_id = pini1.project_innovation_id and pico.id_phase = pini1.id_phase 
			INNER JOIN institutions io on io.id = pico.institution_id 
			) inst on inst.project_innovation_id = pini.project_innovation_id and inst.id_phase = pini.id_phase 
-- group shared clusters		   
left join (select pis2.id_phase , pes2.id inno_id, group_concat(pi4.acronym SEPARATOR ',') shared_clusters_acronym
from project_innovations pes2  
inner JOIN project_innovation_shared pis2 on pis2.project_innovation_id = pes2.id and pis2.is_active   
inner join projects pps on pis2.project_id = pps.id and pps.is_active 
inner join projects_info pi4 on pi4.project_id = pis2.project_id and pi4.id_phase = pis2.id_phase and pi4.is_active 
join bi_parameters bp12 on bp12.parameter_name ='BI_data_phase'
where pes2.is_active 
and pes2.project_id != pis2.project_id 
and pis2.id_phase = cast(bp12.parameter_value as double)
group by pis2.id_phase , pes2.id
) sclu on sclu.id_phase = pini.id_phase and sclu.inno_id = pini.project_innovation_id  			
WHERE ph.id = cast(bpacc.parameter_value as double)
-- ph.global_unit_id = 45
AND pin.is_active
-- AND ph.description = "Reporting"
-- AND ph.`year` = 2023
-- AND pini.`year` = 2022
-- GROUP BY
-- 	pin.id
ORDER BY
-- 	p.id,
	pin.id
;	
</sql>
