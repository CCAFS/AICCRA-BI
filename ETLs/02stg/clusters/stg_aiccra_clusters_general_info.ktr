    <connection>ORI_AICCRA</connection>
    <sql>select pi2.project_id cluster_id , 
CONCAT('C',pi2.project_id)  cluster_compose_id,
pi2.id_phase , pi2.title cluster_title, p.acronym cluster_acronym ,
pi2.summary cluster_summary, pi2.type_id , ct.name cluster_type , 
pi2.liaison_institution_id, 
i.name liaison_institution_name1,  
li.name liaison_institution_name2,
CONCAT('https://aiccra.marlo.cgiar.org/clusters/AICCRA/description.do?projectID=',pi2.project_id ,'&amp;edit=true&amp;phaseID=',miph.max_id_phase) cluster_link,
miph.max_id_phase,
now() updated_date
from projects p 
join projects_info pi2 on pi2.project_id = p.id 
INNER JOIN (select parameter_name, cast(parameter_value as double) parameter_value from bi_parameters where parameter_name='BI_data_phase' ) bpacc on bpacc.parameter_name= 'BI_data_phase'
INNER JOIN phases ph ON ph.id = pi2 .id_phase 
INNER JOIN global_units gu ON gu.id = ph.global_unit_id AND gu.is_active
INNER JOIN liaison_institutions li on li.id = pi2.liaison_institution_id 
inner join institutions i on i.id = li.institution_id 
INNER JOIN (select gu1.id gu_id, phg.id max_id_phase, phg.name name_max_id_phase , phg.year year_max_id_phase, phg.description desc_max_id_phase
from global_units gu1 
join phases phg on phg.global_unit_id = gu1.id 
where gu1.is_active
and phg.id = ifnull(get_max_open_phase(gu1.id),0)
) miph on miph.gu_id = gu.id
left join cluster_types ct on ct.id = pi2.type_id
where ph.id = bpacc.parameter_value
;
</sql>
